{
    "name": "mobianglobal/mobian-api-php",
    "description": "MOBIAN Global API package for PHP.",
    "license": "MIT",
    "homepage": "https://mobian.global/",
    "authors": [
        {
            "name": "Lars Grevelink",
            "homepage": "https://github.com/larsgrevelink"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mobian\\ApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "minimum-stability": "stable"
}
